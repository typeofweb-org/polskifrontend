{
  "name": "polskifrontend",
  "version": "4.6.1",
  "np": {
    "branch": "main",
    "releaseDraft": true
  },
  "repository": {
    "url": "https://github.com/typeofweb/polskifrontend"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --names api,_db,www --prefix-colors yellow,green,blue.inverse 'yarn dev:start:api' 'yarn dev:start:db' 'yarn dev:start:www'",
    "dev:start:api": "wait-on tcp:5438 --interval 5000 && yarn db:migrate:up",
    "dev:start:db": "supabase start",
    "dev:stop:db": "supabase stop",
    "dev:start:www": "next dev",
    "db:migrate:up": "prisma migrate deploy --preview-feature && prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "test:ci": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "studio": "prisma studio",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "./api-helpers/prisma/schema.prisma"
  },
  "author": "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "contributors": [
    "Bartłomiej Dybowski <b.dybowski@gmail.com> (https://www.nafrontendzie.pl/)",
    "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)"
  ],
  "license": "AGPL-3.0-only",
  "private": true,
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "@hcaptcha/react-hcaptcha": "1.1.1",
    "@prisma/client": "3.12.0",
    "@supabase/supabase-js": "1.33.3",
    "@supabase/ui": "0.36.5",
    "algoliasearch": "4.13.0",
    "cheerio": "1.0.0-rc.10",
    "cloudinary": "1.29.0",
    "clsx": "1.1.1",
    "feed": "4.2.2",
    "feedparser": "2.2.10",
    "googleapis": "100.0.0",
    "hcaptcha": "0.1.1",
    "html-entities": "2.3.3",
    "iconv-lite": "0.6.3",
    "mailgun-js": "0.22.0",
    "ms": "2.1.3",
    "next": "12.1.4",
    "next-seo": "5.4.0",
    "next-superjson": "0.0.2",
    "normalize.css": "8.0.1",
    "pino": "7.10.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-instantsearch-dom": "6.23.3",
    "rxjs": "7.5.5",
    "slugify": "1.6.5",
    "superjson": "1.8.1",
    "xss": "1.0.11",
    "yup": "1.0.0-beta.3"
  },
  "devDependencies": {
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "14.0.4",
    "@typeofweb/eslint-plugin": "0.3.0",
    "@types/cheerio": "0.22.31",
    "@types/feedparser": "2.2.5",
    "@types/gtag.js": "0.0.10",
    "@types/hcaptcha__react-hcaptcha": "0.1.5",
    "@types/jest": "27.4.1",
    "@types/mailgun-js": "0.22.12",
    "@types/ms": "0.7.31",
    "@types/node": "17.0.23",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-instantsearch-dom": "6.12.2",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "all-contributors-cli": "6.20.0",
    "concurrently": "7.1.0",
    "critters": "0.0.16",
    "cross-env": "7.0.3",
    "dotenv": "16.0.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "msw": "0.39.2",
    "pino-pretty": "7.6.1",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "rimraf": "3.0.2",
    "sass": "1.50.0",
    "supabase": "0.5.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "typescript-plugin-css-modules": "3.4.0",
    "wait-on": "6.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --resolve-plugins-relative-to . --fix",
      "yarn prettier --write"
    ],
    "*.{json,md,yaml,yml,scss,css}": [
      "yarn prettier --write"
    ]
  }
}
