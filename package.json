{
  "name": "polskifrontend",
  "scripts": {
    "dev": "yarn --frozen-lockfile && concurrently --kill-others-on-fail --names api,_db,www --prefix-colors yellow,green,blue.inverse 'yarn dev:start:api' 'yarn dev:start:db' 'yarn dev:start:www'",
    "dev:start:api": "wait-on tcp:5432 --interval 5000 && yarn db:migrate:up",
    "dev:start:db": "docker-compose up",
    "dev:start:www": "next dev",
    "db:migrate:up": "prisma migrate dev --preview-feature && prisma generate",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "test:ci": "jest",
    "tsc": "tsc --noEmit"
  },
  "prisma": {
    "schema": "./api-helpers/prisma/schema.prisma"
  },
  "author": "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "contributors": [
    "Bartłomiej Dybowski <b.dybowski@gmail.com> (https://www.nafrontendzie.pl/)",
    "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)"
  ],
  "license": "AGPL-3.0-only",
  "private": true,
  "dependencies": {
    "@hapi/boom": "9.1.0",
    "@prisma/client": "2.13.0",
    "@types/hapi__boom": "9.0.1",
    "clsx": "1.1.1",
    "feedparser": "2.2.10",
    "iconv-lite": "0.6.2",
    "ms": "2.1.3",
    "next": "10.0.3",
    "next-images": "1.6.2",
    "pino": "6.8.0",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-query": "3.2.0",
    "rxjs": "6.6.3",
    "slugify": "1.4.6",
    "tunnel-ssh": "4.1.4",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@prisma/cli": "2.13.0",
    "@swc-node/jest": "1.0.2",
    "@testing-library/dom": "7.29.0",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@types/feedparser": "2.2.3",
    "@types/jest": "26.0.19",
    "@types/ms": "0.7.31",
    "@types/node": "14.14.13",
    "@types/pino": "6.3.4",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/testing-library__react": "10.2.0",
    "@types/tunnel-ssh": "4.1.0",
    "@types/yup": "0.29.10",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-functional": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "jest-css-modules": "2.1.0",
    "jest-extended": "0.11.5",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.3",
    "mongodb": "3.6.3",
    "msw": "0.24.2",
    "node-sass": "5.0.0",
    "pino-pretty": "4.3.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "react-query-devtools": "2.6.3",
    "rimraf": "3.0.2",
    "ts-loader": "8.0.12",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "typescript-plugin-css-modules": "3.0.1",
    "wait-on": "5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "yarn prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "yarn prettier --write"
    ]
  }
}
