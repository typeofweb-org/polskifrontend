{
  "name": "polskifrontend",
  "version": "4.6.0",
  "np": {
    "branch": "main",
    "releaseDraft": true
  },
  "repository": {
    "url": "https://github.com/typeofweb/polskifrontend"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --names api,_db,www --prefix-colors yellow,green,blue.inverse 'yarn dev:start:api' 'yarn dev:start:db' 'yarn dev:start:www'",
    "dev:start:api": "wait-on tcp:5432 --interval 5000 && yarn db:migrate:up",
    "dev:start:db": "supabase start",
    "dev:stop:db": "supabase stop",
    "dev:start:www": "next dev",
    "db:migrate:up": "prisma migrate deploy --preview-feature && prisma generate",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint --resolve-plugins-relative-to . --ext .js,.jsx,.ts,.tsx --fix .",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "test:ci": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "studio": "prisma studio",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "./api-helpers/prisma/schema.prisma"
  },
  "author": "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "contributors": [
    "Bartłomiej Dybowski <b.dybowski@gmail.com> (https://www.nafrontendzie.pl/)",
    "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)"
  ],
  "license": "AGPL-3.0-only",
  "private": true,
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "@hcaptcha/react-hcaptcha": "0.3.7",
    "@prisma/client": "3.2.1",
    "@sentry/browser": "6.13.3",
    "@sentry/integrations": "6.13.3",
    "@sentry/node": "6.13.3",
    "@sentry/react": "6.13.3",
    "@sentry/tracing": "6.13.3",
    "@sentry/webpack-plugin": "1.17.3",
    "@supabase/supabase-js": "1.24.0",
    "@supabase/ui": "0.35.0",
    "algoliasearch": "4.10.5",
    "cheerio": "1.0.0-rc.10",
    "cloudinary": "1.27.0",
    "clsx": "1.1.1",
    "feed": "4.2.2",
    "feedparser": "2.2.10",
    "googleapis": "88.2.0",
    "hcaptcha": "0.1.0",
    "html-entities": "2.3.2",
    "iconv-lite": "0.6.3",
    "mailgun-js": "0.22.0",
    "ms": "2.1.3",
    "next": "11.1.3",
    "next-seo": "4.28.1",
    "normalize.css": "8.0.1",
    "pino": "6.13.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-instantsearch-dom": "6.12.1",
    "rxjs": "7.4.0",
    "slugify": "1.6.1",
    "superjson": "1.7.5",
    "xss": "1.0.10",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.1.2",
    "@swc-node/jest": "1.3.2",
    "@testing-library/dom": "8.7.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.2.1",
