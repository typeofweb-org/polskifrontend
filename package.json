{
  "name": "polskifrontend",
  "version": "3.14.0",
  "np": {
    "branch": "main",
    "releaseDraft": true
  },
  "repository": {
    "url": "https://github.com/typeofweb/polskifrontend"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --names api,_db,www --prefix-colors yellow,green,blue.inverse 'yarn dev:start:api' 'yarn dev:start:db' 'yarn dev:start:www'",
    "dev:start:api": "wait-on tcp:5432 --interval 5000 && yarn db:migrate:up",
    "dev:start:db": "docker-compose up",
    "dev:start:www": "next dev",
    "db:migrate:up": "prisma migrate deploy --preview-feature && prisma generate",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "test:ci": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "studio": "prisma studio"
  },
  "prisma": {
    "schema": "./api-helpers/prisma/schema.prisma"
  },
  "author": "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "contributors": [
    "Bartłomiej Dybowski <b.dybowski@gmail.com> (https://www.nafrontendzie.pl/)",
    "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)"
  ],
  "license": "AGPL-3.0-only",
  "private": true,
  "dependencies": {
    "@hapi/boom": "9.1.3",
    "@hcaptcha/react-hcaptcha": "0.3.6",
    "@prisma/client": "2.26.0",
    "@sentry/browser": "6.8.0",
    "@sentry/integrations": "6.8.0",
    "@sentry/node": "6.8.0",
    "@sentry/react": "6.8.0",
    "@sentry/tracing": "6.8.0",
    "@sentry/webpack-plugin": "1.16.0",
    "@supabase/supabase-js": "1.18.1",
    "@supabase/ui": "0.28.1",
    "algoliasearch": "4.10.3",
    "cheerio": "1.0.0-rc.10",
    "clsx": "1.1.1",
    "feed": "4.2.2",
    "feedparser": "2.2.10",
    "hcaptcha": "0.0.2",
    "html-entities": "2.3.2",
    "iconv-lite": "0.6.3",
    "mailgun-js": "0.22.0",
    "ms": "2.1.3",
    "next": "11.0.1",
    "next-seo": "4.26.0",
    "normalize.css": "8.0.1",
    "pino": "6.11.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-instantsearch-dom": "6.12.0",
    "rxjs": "7.2.0",
    "slugify": "1.5.3",
    "superjson": "1.7.4",
    "xss": "1.0.9",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.0.1",
    "@swc-node/jest": "1.3.1",
    "@testing-library/dom": "8.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/cheerio": "0.22.30",
    "@types/feedparser": "2.2.4",
    "@types/gtag.js": "0.0.7",
    "@types/hcaptcha__react-hcaptcha": "0.1.5",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.171",
    "@types/mailgun-js": "0.22.12",
    "@types/ms": "0.7.31",
    "@types/node": "14.14.37",
    "@types/pino": "6.3.9",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-instantsearch-dom": "6.10.2",
    "@types/yup": "0.29.13",
    "@types/zeit__next-source-maps": "0.0.2",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "all-contributors-cli": "6.20.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.6",
    "babel-plugin-superjson-next": "0.3.0",
    "concurrently": "6.2.0",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-flowtype": "5.8.0",
    "eslint-plugin-functional": "3.2.1",
