    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "jest": "27.0.6",
    "jest-css-modules": "2.1.0",
    "jest-extended": "0.11.5",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.0.0",
    "lodash": "4.17.21",
    "msw": "0.31.0",
    "pino-pretty": "5.1.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.2",
    "prisma": "2.26.0",
    "rimraf": "3.0.2",
    "sass": "1.35.2",
    "ts-loader": "9.2.3",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "typescript-plugin-css-modules": "3.4.0",
    "wait-on": "6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "yarn prettier --write"
    ],
    "*.{json,md,yaml,yml,scss,css}": [
      "yarn prettier --write"
    ]
  }
}
