{
  "name": "polskifrontend",
  "version": "3.10.1",
  "np": {
    "branch": "main",
    "releaseDraft": true
  },
  "repository": {
    "url": "https://github.com/typeofweb/polskifrontend"
  },
  "scripts": {
    "dev": "yarn --frozen-lockfile && concurrently --kill-others-on-fail --names api,_db,www --prefix-colors yellow,green,blue.inverse 'yarn dev:start:api' 'yarn dev:start:db' 'yarn dev:start:www'",
    "dev:start:api": "wait-on tcp:5432 --interval 5000 && yarn db:migrate:up",
    "dev:start:db": "docker-compose up",
    "dev:start:www": "next dev",
    "db:migrate:up": "prisma migrate deploy --preview-feature && prisma generate",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest --detectOpenHandles --forceExit --passWithNoTests",
    "test:ci": "jest --passWithNoTests",
    "tsc": "tsc --noEmit",
    "studio": "prisma studio",
    "deploy:heroku": "git subtree push --prefix heroku heroku master"
  },
  "prisma": {
    "schema": "./api-helpers/prisma/schema.prisma"
  },
  "author": "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "contributors": [
    "Bartłomiej Dybowski <b.dybowski@gmail.com> (https://www.nafrontendzie.pl/)",
    "Michał Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)"
  ],
  "license": "AGPL-3.0-only",
  "private": true,
  "dependencies": {
    "@hapi/boom": "9.1.1",
    "@hcaptcha/react-hcaptcha": "0.3.2",
    "@prisma/client": "2.17.0",
    "@sentry/browser": "6.2.0",
    "@sentry/integrations": "6.2.0",
    "@sentry/node": "6.2.0",
    "@sentry/react": "6.2.0",
    "@sentry/tracing": "6.2.0",
    "@sentry/webpack-plugin": "1.14.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "algoliasearch": "4.8.5",
    "cheerio": "1.0.0-rc.5",
    "clsx": "1.1.1",
    "feed": "4.2.2",
    "feedparser": "2.2.10",
    "hcaptcha": "0.0.2",
    "html-entities": "2.1.0",
    "iconv-lite": "0.6.2",
    "mailgun-js": "0.22.0",
    "ms": "2.1.3",
    "next": "10.0.7",
    "next-auth": "3.5.1",
    "next-images": "1.7.0",
    "next-seo": "4.20.0",
    "normalize.css": "8.0.1",
    "pino": "6.11.1",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-instantsearch-dom": "6.10.0",
    "rxjs": "6.6.3",
    "slugify": "1.4.7",
    "superjson": "1.7.2",
    "xss": "1.0.8",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.0.7",
    "@prisma/cli": "2.17.0",
    "@swc-node/jest": "1.1.0",
    "@testing-library/dom": "7.29.6",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.7.3",
    "@types/cheerio": "0.22.24",
    "@types/feedparser": "2.2.3",
    "@types/gtag.js": "0.0.4",
    "@types/hcaptcha__react-hcaptcha": "0.1.4",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/mailgun-js": "0.22.11",
    "@types/mongodb": "3.6.8",
    "@types/ms": "0.7.31",
    "@types/next-auth": "3.1.24",
    "@types/node": "14.14.31",
    "@types/pino": "6.3.6",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-instantsearch-dom": "6.8.0",
    "@types/yup": "0.29.11",
    "@types/zeit__next-source-maps": "0.0.2",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "all-contributors-cli": "6.20.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-superjson-next": "0.2.2",
    "concurrently": "6.0.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.0.0",
