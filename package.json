    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "jest-css-modules": "2.1.0",
    "jest-extended": "0.11.5",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "11.2.2",
    "lodash": "4.17.21",
